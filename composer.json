{
  "name": "contributte/forms-wizard",
  "type": "library",
  "description": "Wizard component for nette/forms",
  "keywords": [
    "nette",
    "contributte",
    "forms",
    "wizard"
  ],
  "require": {
    "php": ">=7.4",
    "nette/forms": "^3.0",
    "nette/http": "^3.0"
  },
  "require-dev": {
    "codeception/codeception": "^4.1.20",
    "codeception/module-asserts": "^1.3.1|^2.0",
    "codeception/module-phpbrowser": "^1.0.2|^2.0",
    "latte/latte": "^2.7|^3.0",
    "nette/application": "~3.1.4",
    "nette/di": "^3.0",
    "nette/tester": "^2.3.2",
    "ninjify/nunjuck": "^0.3",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0",
    "webchemistry/testing-helpers": "^3.0.0"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Contributte\\FormWizard\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/includes"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.2.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
