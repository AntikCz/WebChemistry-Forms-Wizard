{
  "name": "contributte/forms-wizard",
  "type": "library",
  "description": "Wizard component for nette/forms",
  "keywords": [
    "nette",
    "contributte",
    "forms",
    "wizard"
  ],
  "require": {
    "php": ">=8.0",
    "nette/forms": "^3.0",
    "nette/http": "^3.0"
  },
  "require-dev": {
    "latte/latte": "^2.7|^3.0",
    "nette/application": "~3.1.4",
    "nette/di": "^3.0",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1",
    "mockery/mockery": "^1.6.7"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Contributte\\FormWizard\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "4.1.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
